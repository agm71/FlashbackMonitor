					<Button FontSize="14" BorderBrush="Gray" Height="32" CornerRadius="0" HorizontalAlignment="Right" Content="&#11166;" Click="NextPageButton_Click"/>
				</StackPanel>
			</DockPanel>
			<Separator Background="{DynamicResource Separator_Background}" Height="5" Margin="22 0 22 10"/>
